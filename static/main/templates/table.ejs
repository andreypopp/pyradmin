<table class="table table-striped table-condensed">
	<thead>
		<% for (var i in fields) { %>
		<th><%= escape(fields[i].name) %></th>
		<% } %>
	</thead>
	<tbody>
		<% for (i in rows) { %>
		<tr>
			<% var row = rows[i]; %>
			<% for (var j in row) { %>
				<td><%= escape(row[j]) %></td>
			<% } %>
		</tr>
		<% } %>
	</tbody>
</table>